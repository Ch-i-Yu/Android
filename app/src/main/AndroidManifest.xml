<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <!-- Access the network for map-related business data requests, including map data, route planning, POI retrieval, etc. -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- Obtain network status and perform data request network conversion according to network status switching -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <!-- Read external storage. If the developer uses the so dynamic loading function and puts the so file in the external storage area, you need to apply for the permission, otherwise you do not need -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:maxSdkVersion="32" />
    <!-- Write to external storage. If the developer uses an offline map and the data is written to an external storage area, you need to apply for this permission -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:maxSdkVersion="32"
        tools:ignore="ScopedStorage" />

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name=".DemoApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CampusRunCW2"
        tools:targetApi="31">
        <meta-data
            android:name="com.baidu.lbsapi.API_KEY"
            android:value="UqyCNvdo0ivbulQe960l9drIcmRtsKdt" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name="com.baidu.location.f"
            android:enabled="true"
            android:process=":remote"/>
    </application>
</manifest>